nama_lengkap = "Abdul Rachman Rofel" #string
print("Print All Text : {0}".format(nama_lengkap))
print("Value indek ke 9 : {0}".format(nama_lengkap[9]))
print("End value in index ke 8 :{0}".format(nama_lengkap[:8]))
print("Start value in index ke 9 : {0}".format(nama_lengkap[9:]))
print("Value index ke 9 sebanyak 10 kali : {0}".format(nama_lengkap[9] * 10))
print("Menggabungkan 2 string : {0}".format(nama_lengkap + " Status mahasiswa"))